  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "period = 5\n",
    "alpha = 2/(1+period)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(1,len(series)):\n",
    "    series.loc[i,name+\"_ewma_unadj\"]=(1-alpha)*series.loc[i-1,name+\"_ewma_unadj\"] + \\\n",
    "    alpha * series.loc[i ,name]\n",
    "    \n",
    "    ajusted_weights = np.array([(1-alpha)**(i-t) for t in range(i+1)])\n",
    "    series.loc[i,name+\"_ewma_adj\"] =np.sum(series.iloc[0:i+1][name].values * ajusted_weights)\\\n",
    "    / ajusted_weights.sum()"
   ]
  },
  {
